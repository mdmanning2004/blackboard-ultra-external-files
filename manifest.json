{
  "name": "Blackboard Ultra External Links",
  "description": "Adds an 'Open in new tab' button to blackboards new file viewer.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "images/external-link.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://vle.shef.ac.uk/ultra/courses/*"
      ]
    }
  ]
}