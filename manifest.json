{
  "name": "Blackboard Ultra External File Viewing",
  "description": "Opens any Blackboard Ultra files in new tabs",
  "version": "1.0.4",
  "manifest_version": 3,
  "action": {
    "default_icon": "images/external-link.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://vle.shef.ac.uk/ultra/courses/*"
      ]
    }
  ]
}