{
  "name": "Blackboard Ultra External File Viewing",
  "description": "Opens any Blackboard Ultra files in new tabs",
  "version": "1.0.4",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon/16.png",
    "32": "images/icon/32.png",
    "48": "images/icon/48.png",
    "128": "images/icon/128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://vle.shef.ac.uk/ultra/courses/*"
      ]
    }
  ]
}